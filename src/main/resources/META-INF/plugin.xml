<idea-plugin>
  <id>ee.veikokaap.idea.plugins.netbeans-plugin-development-from-intellij</id>
  <name>NetBeans IDE plugin development from IntelliJ</name>

  <idea-version since-build="181.5281" until-build="999.*"/>

  <description><![CDATA[
    IntelliJ IDEA plugin for aiding development of NetBeans IDE plugins.<br><br>
    ]]></description>

  <!--More information at <a href="https://github.com/veikokaap/eclipse-plugin-dev-from-intellij">GitHub</a>.-->

  <depends>org.jetbrains.idea.maven</depends>

  <extensions defaultExtensionNs="com.intellij">
    <configurationType implementation="ee.veikokaap.idea.plugins.netbeans.NetBeansPluginConfigurationType"/>
  </extensions>

</idea-plugin>